<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />

	<bean id="school" class="com.bridgelabz.autowiringusingannotation.School" >
		<property name="SVM"></property>
	</bean>
	<bean id="student1" class="com.bridgelabz.autowiringusingannotation.Student">
		<property name="name" value="java book"></property>
		<property name="id" value="225" />
		<property name="email" value="Java Book@gmial.com" />
	</bean>

</beans>