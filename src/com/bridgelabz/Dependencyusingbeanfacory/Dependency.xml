<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="e" class="com.bridgelabz.springsexamples.Employee" scope="prototype">  
<constructor-arg  value="20" index="0"/>
<constructor-arg  value="Student" index="1"/>
</bean>   

 <bean id="e1" class="com.bridgelabz.springsexamples.Employee" scope="prototype"> 
<property name="id" value="100"></property>
<property name="name" value="shruti"></property>
<property name="map">
<map>  
<entry key="Java is a Programming Language"  value="Ajay Kumar"></entry>  
<entry key="Java is a Platform" value="John Smith"></entry>  
<entry key="Java is an Island" value="Raj Kumar"></entry>  
</map>  
</property>
</bean> 
</beans>  